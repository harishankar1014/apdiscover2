[Unit]
Description=APDiscoverLogin
After=network.target
[Service]
ExecStart=/home/ubuntu/apdiscover/apdiscover_node/UserManagement/userManagement.js
Restart=always
User=ubuntu
Group=ubuntu
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production
WorkingDirectory=/home/ubuntu/apdiscover/apdiscover_node/UserManagement
[Install]
WantedBy=multi-user.target